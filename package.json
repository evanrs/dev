{
  "name": "dev",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*",
    "support/*",
    "tools/*"
  ],
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "buildup": "app=$0; tool=$1; echo \nbuilding: $tool && yarn workspace \"$tool\" build && echo \nupdating: $app && yarn workspace \"$app\" add \"$tool\" && echo \"Build and loaded $tool into $app\""
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "gts": "^3.1.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@dev/preset": "workspace:^"
  },
  "volta": {
    "node": "16.14.0"
  }
}
